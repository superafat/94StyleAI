# Google Gemini API
# 获取方式: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Replicate API
# 获取方式: https://replicate.com/account/api-token
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Firebase 配置
# 获取方式: https://console.firebase.google.com/project/94style-ai/settings/general
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=94style-ai.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=94style-ai
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=94style-ai.firebasestorage.app
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_firebase_app_id

# Firebase Admin SDK (用于服务端)
FIREBASE_ADMIN_PRIVATE_KEY=your_firebase_admin_private_key
FIREBASE_ADMIN_CLIENT_EMAIL=your_firebase_admin_client_email
